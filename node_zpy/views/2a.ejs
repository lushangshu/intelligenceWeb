<script>
  function filter2a() {
    var text1 = document.getElementById("2a1").value;
    var text2 = document.getElementById("2a2").value;
    var text3 = document.getElementById("2a3").value;

    for (i = 0; i < text1.split(",").length; i++) {
      var text11 = text1.split(",")[i];
      if ((text11.replace(/\s/g, "")).length == 0) {
        alert("Name not in format");
        return false;
        break;
      }
    }

    if ((text1.split(",")).length > 2) {
      alert("Name out if range");
      return false;
    }

    else if (isNaN(text2) || (parseInt(text2) != text2)) {
      alert("KW not INT!");
      return false;
    }

    else if ((text2 < 1) || (text2 > 7)) {
      alert("KW out of range!");
      return false;
    }

    else if (isNaN(text3) || (parseInt(text3) != text3)) {
      alert("Days not INT!");
      return false;
    }

    else if ((text3 < 1) || (text3 > 7)) {
      alert("Days out of range!");
      return false;
    }
  }
</script>

